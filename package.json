{
  "name": "ci-ally",
  "version": "1.0.0",
  "description": "Help new contributors understand and fix failing builds.",
  "author": "Nick Cannariato <434063+nickcannariato@users.noreply.github.com> (https://github.com/nickcannariato/ci-ally)",
  "license": "ISC",
  "repository": "https://github.com//ci-ally.git",
  "homepage": "https://github.com//ci-ally",
  "bugs": "https://github.com//ci-ally/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "probot": "^12.2.8"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
